{
  "_version": "1.49.0",
  "sap.app": {
    "id": "myswd.swd.zlilovion",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.9.6",
      "toolsId": "d169ba35-b217-49ff-8fb8-0bbc24ff078b"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.108.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "myswd.swd.zlilovion.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "myswd.swd.zlilovion.view",
        "controlAggregation": "pages",
        "controlId": "app-flex-layout"
      },
      "routes": [
        {
          "name": "MainScreen",
          "pattern": "",
          "target": [
            "MainScreen"
          ]
        },
        {
          "name": "Kundenauftrag",
          "pattern": "Kundenauftrag",
          "target": [
            "Kundenauftrag"
          ]
        },
        {
          "name": "Revision",
          "pattern": "Revision",
          "target": [
            "Revision"
          ]
        },
        {
          "name": "KundenDetailScreen",
          "pattern": "KundenDetailScreen",
          "target": [
            "KundenDetailScreen"
          ]
        },
        {
          "name": "InstaDetailScreen",
          "pattern": "InstaDetailScreen",
          "target": [
            "InstaDetailScreen"
          ]
        },

        {
          "name": "Instandhaltungsauftrag",
          "pattern": "Instandhaltungsauftrag",
          "target": [
            "Instandhaltungsauftrag"
          ]
        }
      ],
      "targets": {
        "MainScreen": {
          "viewType": "XML",
          "transition": "slide",
          "controlAggregation": "beginColumnPages",
          "clearControlAggregation": true,
          "viewId": "idMainScreen",
          "viewName": "MainScreen"
        },
        "Instandhaltungsauftrag": {
          "viewType": "XML",
          "transition": "slide",
          "controlAggregation": "beginColumnPages",
          "clearControlAggregation": true,
          "viewId": "idInstandhaltungsauftrag",
          "viewName": "Instandhaltungsauftrag"
        },
        "Revision": {
          "viewType": "XML",
          "transition": "slide",
          "controlAggregation": "beginColumnPages",
          "clearControlAggregation": true,
          "viewId": "idRevision",
          "viewName": "Revision"
        },
        "Kundenauftrag": {
          "viewType": "XML",
          "transition": "slide",
          "controlAggregation": "beginColumnPages",
          "clearControlAggregation": true,
          "viewId": "idKundenauftrag",
          "viewName": "Kundenauftrag"
        },
        "KundenDetailScreen": {
          "viewType": "XML",
          "transition": "slide",
          "controlAggregation": "beginColumnPages",
          "clearControlAggregation": true,
          "viewId": "idKundenDetailScreen",
          "viewName": "KundenDetailScreen"
        },
        "InstaDetailScreen": {
          "viewType": "XML",
          "transition": "slide",
          "controlAggregation": "beginColumnPages",
          "clearControlAggregation": true,
          "viewId": "idInstaDetailScreen",
          "viewName": "InstaDetailScreen"
        }
      }
    },
    "rootView": {
      "viewName": "myswd.swd.zlilovion.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
